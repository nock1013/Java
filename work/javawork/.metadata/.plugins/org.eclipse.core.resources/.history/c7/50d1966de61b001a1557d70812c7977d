package oop.chap06;

import java.util.Iterator;

// 메서드의 매개변수로 기본형과 참조형을 정의하는 경우 차이점
public class CallByTest {
	public static void main(String[] args) {
		CallByTest obj = new CallByTest();
		int i = 100;
		int[] myarr = new int[5];
		
		for (int j = 0; j < myarr.length; j++) {
			myarr[j] = 100;
		}
		System.out.println("======== 변경 전 ========");
		

	}
	public 

}

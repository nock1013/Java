package oop.chap07;

public class Person {
	String name;
	int age;
	
	public Person() {
		
	}

	protected String getName() {
		return name;
	}
	protected void setName(String name) {
		this.name = name;
	}
	protected int getAge() {
		return age;
	}
	protected void setAge(int age) {
		this.age = age;
	}
	
	
}

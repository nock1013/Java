package oop.chap07.poly;
// 객체의 형변환 - 상속관계에 있는 경우에만 가능하다.
class Parent {
	public void display() {
		System.out.println("부모의 display");
	}
}
class Child extends Parent {
	public void display() {
		System.out.println("자식의 display");
	}
}
public class RefCastingTest {
	public static void main(String[] args) {
		
	}
}

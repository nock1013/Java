package chap05;

public class Array2DExam01 {

	public static void main(String[] args) {	
		int[][] arr = new int[5][5];
		
		for(int i = 0; i < arr.length; i++)
		{
			for(int j = 0; j < arr[i].length; j++)
			{
				arr[i][j] = 5 * i + (j + 1) ;
			}
		}
		
		for(int i = 0; i < arr.length; i++)
		{
			for(int j = 0; j < arr[i].length; j++)
			{
				System.out.println(arr[i][j]);
			}
		}
		
		

	}

}

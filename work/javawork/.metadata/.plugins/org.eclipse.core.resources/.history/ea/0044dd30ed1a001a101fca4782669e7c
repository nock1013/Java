package oop.chap06;

public class PersonTest {

	public static void main(String[] args) {
		Person p1 = new Person(); // 객체 또는 인스턴스
		p1.name = "장동건";
		p1.address = "서울시";
		p1.age = 45;

		System.out.println("성명: " + p1.name);
		System.out.println("주소: " + p1.address);
		System.out.println("나이: " + p1.age);
		
		Person p2 = new Person(); // 객체 또는 인스턴스
		p2.name = "장동건";
		p2.address = "서울시";
		p2.age = 45;

		System.out.println("성명: " + p2.name);
		System.out.println("주소: " + p2.address);
		System.out.println("나이: " + p2.age);
	}

}

package oop.chap07;

public class Staff extends Person {
	private String dept;
	
	public Staff() {
		
	}
	public Staff(String name, int age, String dept) {
		super();
		this.name = name;
		this.age = age;
		this.dept = dept;
	}
	
	public String getDept() {
		return dept;
	}
	public void setDept(String dept) {
		this.dept = dept;
	}
	
	public void print() {
		System.out.println("성명: " + getName()+ " 나이: " + getAge() + " 부서: "+ getDept());
	}
	
}

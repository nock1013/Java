package workbook;
public class Student01 {
	private String name;
	private int age;
	private int height;
	private int weight;
	
	public Student01() {
		
	}

	public Student01(String name, int age, int height, int weight) {
		this.name = name;
		this.age = age;
		this.height = height;
		this.weight = weight;
	}
	
}

package oop.chap06;

public class Test06 {

	public static void main(String[] args) {
		Student[] students = new Student[3];
		students[0] = new Student("Kim", 100, 90, 95, 89);
		
		students[1].setName("Lee");
		students[1].setKorean(60);
		students[1].setEnglish(70);
		students[1].setMath(99);
		students[1].setScience(98);
		
		students[2].setName("Park");
		students[2].setKorean(68);
		students[2].setEnglish(86);
		students[2].setMath(60);
		students[2].setScience(40);
		
		for(int i = 0; i < students.length; i++) {
			students[i].print();
		}

	}

}

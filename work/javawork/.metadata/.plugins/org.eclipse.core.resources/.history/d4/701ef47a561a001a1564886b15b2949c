package chap05;

import java.util.Random;

public class Array2DExam05 {

	public static void main(String[] args) {
		int[] arr3 = new int[5];
		Random rand = new Random();
		
		for(int i = 0; i < arr3.length; i++)
		{
			arr3[i] = rand.nextInt(10) + 1; 
		}
		
		for(int i = 0; i < arr3.length; i++)
		{
			System.out.print(arr3[i] + "\t");
		}
		
		int sum = 0;
		float avg = 0.f;
		for(int i = 0; i < arr3.length; i++)
		{
			sum += arr3[i];
		}
			
	}

}

package coffee;

public class Beverage {
	private string name;
	protected int price;
	
	public Beverage() {
		
	}
	public Beverage(string name) {
		super();
		this.name = name;
	}
	public string getName() {
		return name;
	}
	public void setName(string name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	
	
}

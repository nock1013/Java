package workbook;

public class L3 extends Car implements Temp{
	
	public L3() {
		
	}
	public L3(String name, String engine, int oilTank, int oilSize, int distance) {
		super(name,engine,oilTank,oilSize,distance);
	}
	@Override
	public int getTempGage() {
		
		return getDistance()/10;
	}
	@Override
	public void go(int distance) {
		setDistance(distance);
		setOilSize(getOilSize()-getDistance()/10);
		
	}
	@Override
	public void setOil(int oliSize) {
		setOilSize(getOilSize()+ oilSize);
		
	}
	public void println() {
		System.out.println("vehicleName"+'\t'+"engineSize"+"\t"+"oilTank"+"\t"+"oilSize"+"\t"+"distance"+"\t"+"temperature");
		System.out.println("-----------------------------------------------------------------------------------------------");
		
	}
	
	

}

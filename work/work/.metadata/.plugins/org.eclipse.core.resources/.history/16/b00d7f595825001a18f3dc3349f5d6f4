package jdbc.basic;
//오라클 접소하기
public class ConnertionTest {

	public static void main(String[] args) {
		//1. 오라클 드라이버 로딩
		try {
			Class.forName("oracle.jdbc.driver.OracleDriver");
			System.out.println("드라이버로딩 성공!!");
		}catch(ClassNotFoundException e) {
			System.out.println("드라이버로딩 실패");
		}

	}
}
